		</add>
		<add as="tcp">
			<mxCell value="TCP" tagName="tcp" vertex="1" style="image;image=static/js/platform/esb/images/48/tcp_request-response.png;">
				<mxGeometry as="geometry" width="75" height="60" />
			</mxCell>
		</add>
		<add as="database">
			<mxCell value="DATABASE" tagName="database" vertex="1" style="image;image=static/js/platform/esb/images/48/database_request-response.png;">
