<mxEditor>
	<ui>
		<add as="graph" element="graph" />
	</ui>
	<Array as="templates">
		<add as="http">
			<mxCell value="HTTP" tagName="http" vertex="1" style="image;image=static/js/platform/esb/images/48/http_request-response.png;">
				<mxGeometry as="geometry" width="75" height="60" />
			</mxCell>
		</add>
		<add as="logger">
			<mxCell value="LOGGER" tagName="logger" vertex="1" style="image;image=static/js/platform/esb/images/48/logger.png;">
