	<mxDefaultPopupMenu as="popupHandler">
		<add as="删除" action="delete" icon="static/js/platform/esb/images/icons/close.gif" if="cell"/>
		<add as="对齐" icon="static/js/platform/esb/images/icons/align.gif" if="cell" >
			<add as="左对齐" action="alignCellsLeft" icon="static/js/platform/esb/images/icons/align.gif" if="cell"/>
			<add as="右对齐" action="alignCellsRight" icon="static/js/platform/esb/images/icons/align.gif" if="cell"/>
			<separator/>
			<add as="上对齐" action="alignCellsTop" icon="static/js/platform/esb/images/icons/align.gif" if="cell"/>
			<add as="下对齐" action="alignCellsBottom" icon="static/js/platform/esb/images/icons/align.gif" if="cell"/>
		</add>
		<add as="格式化" action="formatGraph" icon="static/js/platform/esb/images/icons/align.gif" />
	</mxDefaultPopupMenu>
	<Array as="actions">
		<add as="alignCellsLeft"><![CDATA[
			function (editor)
			{
				editor.graph.alignCells("left");
			}
		]]></add>
		<add as="alignCellsRight"><![CDATA[
			function (editor)
			{
				editor.graph.alignCells("right");
			}
		]]></add>
		<add as="alignCellsTop"><![CDATA[
			function (editor)
			{
				editor.graph.alignCells("top");
			}
		]]></add>
		<add as="alignCellsBottom"><![CDATA[
			function (editor)
			{
				editor.graph.alignCells("bottom");
			}
		]]></add>
		<add as="formatGraph"><![CDATA[
			function (editor)
			{
				myAction.formatGraph();
			}
		]]></add>
	</Array>
</mxEditor>
