			</mxCell>
		</add>
		<add as="flowref">
			<mxCell value="FLOW REFERENCE" tagName="flowref" vertex="1" style="image;image=static/js/platform/esb/images/48/flowref.png;">
				<mxGeometry as="geometry" width="75" height="60" />
			</mxCell>
		</add>
		<add as="choice">
			<mxCell value="CHOICE" tagName="choice" vertex="1" style="swimlane;strokeColor=#9393FF;fillColor=#9393FF;" banTag=",exception,choiceexception,">
				<mxGeometry as="geometry" width="150" height="100"/>
			</mxCell>
		</add>
		<add as="scattergather">
			<mxCell value="PARALLEL PROCESS" tagName="scattergather" vertex="1" style="swimlane;strokeColor=#63B8FF;fillColor=#63B8FF;" banTag=",exception,choiceexception,">
				<mxGeometry as="geometry" width="150" height="100"/>
			</mxCell>
		</add>
		<add as="exception">
			<mxCell value="CATCH EXCEPTION" tagName="exception" vertex="1" style="swimlane;strokeColor=#FFAF60;fillColor=#FFAF60;" connectable="0" banTag=",exception,choiceexception,">
				<mxGeometry as="geometry" width="150" height="100"/>
			</mxCell>
		</add>
		<add as="choiceexception">
			<mxCell value="CHOICE EXCEPTION" tagName="choiceexception" vertex="1" style="swimlane;strokeColor=#C6A300;fillColor=#C6A300;" connectable="0" allowTag=",exception,">
				<mxGeometry as="geometry" width="150" height="100"/>
			</mxCell>
		</add>
	</Array>
	<mxGraph as="graph" dropEnabled="1">
		<mxStylesheet as="stylesheet">
			<add as="defaultVertex" extend="defaultVertex">
				<add as="fillColor" value="#adc5ff" />
				<add as="gradientColor" value="#7d85df" />
				<add as="strokeColor" value="#5d65df" />
				<add as="fontSize" value="12" />
				<add as="fontColor" value="#1d258f" />
				<add as="fontFamily" value="Verdana" />
			</add>
			<add as="defaultEdge" extend="defaultEdge">
				<add as="labelBackgroundColor" value="white" />
				<add as="rounded" value="1" />
				<add as="edgeStyle" value="elbowEdgeStyle" />
				<add as="strokeColor" value="#5d65df" />
				<add as="fontSize" value="11" />
				<add as="fontColor" value="#1d258f" />
				<add as="fontFamily" value="Verdana" />
			</add>
			<add as="image">
				<add as="shape" value="image" />
				<add as="perimeter" value="rectanglePerimeter" />
				<add as="fontSize" value="12" />
				<add as="verticalAlign" value="top" />
				<add as="verticalLabelPosition" value="bottom" />
				<add as="resizable" value="0" />
			</add>
			<add as="swimlane">
				<add as="shape" value="swimlane"/>
				<add as="shadow" value="0"/>
				<add as="startSize" value="23"/>
				<add as="align" value="center"/>
				<add as="verticalAlign" value="top"/>
				<add as="foldable" value="0"/>
				<add as="gradientColor" value="none" />
				<add as="fontColor" value="#FFFFFF" />
			</add>
		</mxStylesheet>
		<mxGraphModel as="model">
			
		</mxGraphModel>
	</mxGraph>
