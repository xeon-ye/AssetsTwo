			</mxCell>
		</add>
		<add as="file">
			<mxCell value="FILE" tagName="file" vertex="1" style="image;image=static/js/platform/esb/images/48/file.png;">
				<mxGeometry as="geometry" width="75" height="60" />
			</mxCell>
		</add>
		<add as="smtp">
			<mxCell value="SMTP" tagName="smtp" vertex="1" style="image;image=static/js/platform/esb/images/48/smtp.png;">
				<mxGeometry as="geometry" width="75" height="60" />
			</mxCell>
		</add>
		<add as="ftp">
			<mxCell value="FTP" tagName="ftp" vertex="1" style="image;image=static/js/platform/esb/images/48/ftp.png;">
