@charset "UTF-8"; 
/*这里主要针对portal自定义复写部分图标使用，所有图标库引用平台功能图标库文件  */
html, body {
	font-family: 'Microsoft YaHei';
	color: #000;
	font-size: 12px
}
a:focus{outline:none;}
@font-face {
	font-family: "iconfont";
	src:
		url('iconfont/iconfont.eot?t=1563260764117');
	/* IE9 */
	src:
		url('iconfont/iconfont.eot?t=1563260764117#iefix')
		format('embedded-opentype'), /* IE6-IE8 */ 
		url('iconfont/iconfont.woff')
		format('woff'),
		url('iconfont/iconfont.woff?t=1563260764117')
		format('woff'),
		url('iconfont/iconfont.ttf?t=1563260764117')
		format('truetype'),
		/* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
		url('iconfont/iconfont.svg?t=1563260764117#iconfont')
		format('svg'); /* iOS 4.1- */
}
/* a:hover,a:active,a:after,a:-webkit-any-link{
	text-decoration: none;
	webkit-transition: background-color .15s linear;
	-moz-transition: background-color .15s linear;
	transition: background-color .15s linear;
} */
.iconfont {
	font-family: "iconfont" !important;
	font-style: normal;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale
}
.glyphicon {
	font-style: normal;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale
}
[class^=icon-] {
	background: 0 0;
	width: auto;
	height: auto;
	line-height: auto
}


.fa-plus:before {
	font-family: "iconfont" !important;
	content: "\e60a"
}

.fa-file-text-o:before {
	font-family: "iconfont" !important;
	content: "\e606"
}

.fa-trash-o:before {
	font-family: "iconfont" !important;
	content: "\e605"
}

.fa-search:before {
	font-family: "iconfont" !important;
	content: "\e6a8"
}

.glyphicon-backward:before {
	font-family: "iconfont" !important;
	content: "\e616"
}

.glyphicon-step-backward:before {
	font-family: "iconfont" !important;
	content: "\e610"
}

.glyphicon-forward:before {
	font-family: "iconfont" !important;
	content: "\e617"
}

.glyphicon-step-forward:before {
	font-family: "iconfont" !important;
	content: "\e613"
}

.layui-layer-min:before {
	font-family: "iconfont" !important;
	content: "\e615"
}

.layui-layer-max:before {
	font-family: "iconfont" !important;
	content: "\e614"
}


.layui-layer-close:before {
	font-family: "iconfont" !important;
	content: "\e60d";
	font-size: 12px
}
 
/* .ztree li span.button.ico_open:before {
	font-family: "iconfont" !important;
	content: "\e6ab"
}

.ztree li span.button.ico_close:before {
	font-family: "iconfont" !important;
	content: "\e6a9"
}

.ztree li span.button.ico_docu:before {
	font-family: "iconfont" !important;
	content: "\e6aa"
}  */
input[type=checkbox]:after {
	font-family: "iconfont" !important;
	content: "\e683";
}

input[type=radio]:after {
	font-family: "iconfont" !important;
	content: "\e665";
}

input[type=checkbox]:checked:after {
	font-family: "iconfont" !important;
	content: "\e662";
}

input[type=radio]:checked:after {
	font-family: "iconfont" !important;
	content: "\e6ac";
}

.icon-edit{
	background:none!important;
}


/*zTree树组件样式 Start*/
.ztree li span.button {
	color:#2fad95;
	line-height: 21px;
	text-align: center;
}
.ztree li span.button:before {
	font-family:"iconfont";
}
.ztree li span.button.ico_open {
	background: none;
	vertical-align: initial;
}
.ztree li span.button.ico_open:before {content: "\e6ab"; }
.ztree li span.button.ico_close {
	background: none;
	vertical-align: initial;
}
.ztree li span.button.ico_close:before {content: "\e6a9"; }
.ztree li span.button.ico_docu {
	background: none;
	vertical-align: initial;
}
.ztree li span.button.ico_docu:before {content: "\e6aa"; }
/*zTree树组件样式 End*/
/* 禁用按钮样式 */
.btn-primary.disabled, .btn-primary:disabled, .btn-primary[disabled] {
    background-color: #efefef;
    background-image: none;
    border:1px solid #e8e8e8;
}
.btn-primary{
	box-shadow:none!important;
}
/* 编码管理弹层管理 */
.panel-success>.panel-heading{
	background-image: -webkit-linear-gradient(top,#ffffff 0,#ffffff 100%);
    background-image: -o-linear-gradient(top,#ffffff 0,#ffffff 100%);
    background-image: -webkit-gradient(linear,left top,left bottom,from(#ffffff),to(#ffffff));
    background-image: linear-gradient(to bottom,#ffffff 0,#ffffff 100%);
	background: #ffffff;
	color: #000000;
    border-color: #dddddd;
}
.panel-success>.panel-heading .panel-title {
    font-size: 14px;
    color: #333333;
}
/* 流程相关样式 */
.cicon{
	color: #cccccc;
	margin:0 5px;
}		
.icon_active{
	color:#1182fa!important;
	margin:0 5px;
}

.dropdown-menu {
	font-size: 12px;
	padding: 0;
}
.dropdown-menu .divider {
    margin: 0;
}
.dropdown-menu>li>a {
    line-height: 28px;
    height: 28px;
    padding: 0 10px;
    color: #484848;
}
.dropdown-menu>li>a:hover{
	background-color: #f5f5f5;
	filter: none;
}
.pslist-table .r{
	width:60%;
}
/* 解决IE8图标样式重绘是使用 */
.content-empty:after,.content-empty:before{
		  content:"" !important;
}

.btn-bar .webuploader-pick {
		background: #2fae95;
}
.btn-bar .webuploader-pick:hover {
		background: #17957c;
}
.inputborder {
    border:1px solid #e8e8e8 !important;
}	

/*复写样式为分页加边框  */
.ui-jqgrid .ui-pg-input,
.ui-jqgrid .ui-pg-selbox{
        border: 1px solid #e8e8e8!important;
}
/* input 相关样式 */
.form-control{
	border:1px solid #e8e8e8;
	height: 32px;
}

.input-group-sm>.form-control, 
.input-group-sm>.input-group-addon, 
.input-group-sm>.input-group-btn>.btn{
	height: 32px;
}
.input-group .form-control:not(:first-child):not(:last-child){
	border-top-left-radius: 3px!important;
    border-bottom-left-radius: 3px!important;
}
/* 解决IE8下select控件选中后文字遮挡问题*/
select.form-control{
	padding-left:5px!important;
	padding-top:5px;
	padding-bottom:5px;
}
.btn .caret{
	margin-left:3px;
}
.form-control[disabled], 
.form-control[readonly], 
fieldset[disabled] .form-control{
	background-color: #fafafa!important;
	color: #666666;
	opacity: 1
}
.input-group-addon{
	background-color: #fafafa!important;
	border-color: #e8e8e8!important;
}
/* 表格相关样式 */
.ui-jqgrid-htable .ui-th-column .ui-th-div .cbox,
.ui-jqgrid td input[type=checkbox]{
	margin:2px 2px 0!important;
}
.ui-jqgrid .ui-jqgrid-btable tbody tr.jqgrow td,
.ui-jqgrid .ui-jqgrid-htable thead th{
	padding-right:2px!important;
}
.ui-jqgrid .ui-jqgrid-htable .ui-th-div{
	line-height: 16px!important;
	font-weight: bold!important;
}
/* 复写表格边框相关样式 */
.ui-jqgrid .ui-jqgrid-hdiv {
	border-top: 1px solid #e8e8e8;
}
.table-bordered {
    border: 1px solid #e8e8e8;
}
.table-bordered>tbody>tr>td, 
.table-bordered>tbody>tr>th, 
.table-bordered>tfoot>tr>td, 
.table-bordered>tfoot>tr>th, 
.table-bordered>thead>tr>td, 
.table-bordered>thead>tr>th {
    border: 1px solid #e8e8e8;
}
.ui-jqgrid-btable td select[multiple], 
.ui-jqgrid-btable td select[size] {
    height: 32px;
    padding-top:4px;
    
}
.edit-cell .form-control{
	font-size:12px;
}
.edit-cell .spinner{
	border:0;
}
/* tab页签样式 */
.nav-tabs {
    border-bottom: 1px solid #e8e8e8;
}
.toolbar .toolbar-left{
	margin-bottom:5px;
}
/* ie8下分页禁用样式 */
 .ui-paging-pager .ui-disabled span.glyphicon,
.ui-paging-pager .ui-disabled span.glyphicon:before{
	color:#a6a6a6\9!important;
} 
/* 附件图标样式开始 */
.uploader-file-item {
    background-color: #ffffff;
    border: 1px solid #e8e8e8;
    border-radius: 3px;
    padding: 10px 6px;
}
.uploader-file-item .uploader-file-ico{
	width:32px;
	height:32px;
	display: block;
}
.uploader-file-item .uploader-file-ico:before{
	content: " ";
}
.uploader-file-item .icon-txt{
	background: url(../../images/platform/common/file/txt-icon.png) no-repeat left top;
}
.uploader-file-item .icon-text{
	background: url(../../images/platform/common/file/text-icon.png) no-repeat left top;
}
.uploader-file-item .icon-word{
	background: url(../../images/platform/common/file/word-icon.png) no-repeat left top;
}
.uploader-file-item .icon-excel{
	background: url(../../images/platform/common/file/excel-icon.png) no-repeat left top;
}
.uploader-file-item .icon-PPT{
	background: url(../../images/platform/common/file/ppt-icon.png) no-repeat left top;
}
.uploader-file-item .icon-PDF{
	background: url(../../images/platform/common/file/pdf-icon.png) no-repeat left top;
}
.uploader-file-item .icon-ZIP{
	background: url(../../images/platform/common/file/zip-icon.png) no-repeat left top;
}
.uploader-file-item .icon-tupian{
	background: url(../../images/platform/common/file/jpg-icon.png) no-repeat left top;
}
.uploader-file-item .icon-shipin{
	background: url(../../images/platform/common/file/video-icon.png) no-repeat left top;
}
.uploader-file-item .icon-mp3{
	background: url(../../images/platform/common/file/mp3-icon.png) no-repeat left top;
}
.uploader-file-item .icon-file1{
	background: url(../../images/platform/common/file/ty-icon.png) no-repeat left top;
}
/* 附件图标样式结束 */



/* 谷歌美化滚动条 */
*::-webkit-scrollbar {
	width: 6px;
	height: 6px;
}
*::-webkit-scrollbar-track {
	background: #f3f4f9;
	border-radius: 2px;
}
*::-webkit-scrollbar-thumb {
	background: #c8d2e0;
	border-radius: 10px;
}
*::-webkit-scrollbar-thumb:hover {
	background: #333;
}
*::-webkit-scrollbar-corner {
	background: #c8d2e0;
}
/*解决83版本以上谷歌浏览器表单控件带边框问题  */
button:focus,select:focus,input:focus,textarea:focus{
	outline: none!important;
}

/*表格列排序及隐藏样式*/
.iconLayerSort{
	font-size: 12px;
	color: #999999;
}
.iconLayerSort:hover{
	color: #1890ff;
}

/* 流程上传附件 */
.uploader-panel-body .uploader-file-list{
	overflow: hidden;
}

